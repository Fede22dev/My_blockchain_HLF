{
  "name": "fablo-test-network-sensors1",
  "description": "Connection profile for Hyperledger Explorer in Fablo network",
  "version": "1.0.0",
  "client": {
    "organization": "Sensors1",
    "tlsEnable": true,
    "enableAuthentication": true,
    "adminCredential": {
      "id": "admin",
      "password": "adminpw"
    },
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "organizations": {
    "Sensors1": {
      "mspid": "Sensors1MSP",
      "adminPrivateKey": {
        "path": "/tmp/crypto/peerOrganizations/sensors1.project.it/users/Admin@sensors1.project.it/msp/keystore/priv-key.pem"
      },
      "peers": [
        "peer0.sensors1.project.it"
      ],
      "signedCert": {
        "path": "/tmp/crypto/peerOrganizations/sensors1.project.it/users/Admin@sensors1.project.it/msp/signcerts/Admin@sensors1.project.it-cert.pem"
      }
    }
  },
  "peers": {
    "peer0.sensors1.project.it": {
      "url": "grpcs://peer0.sensors1.project.it:7101",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/sensors1.project.it/msp/tlscacerts/tlsca.sensors1.project.it-cert.pem"
      }
    }
  },
  "channels": {
    "home1": {
      "peers": {}
    }
  }
}
